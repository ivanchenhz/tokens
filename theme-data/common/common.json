{
  "theme": {
    "common": {
      "text": {
        "white": "@color-white-alpha-95",
        "gray": "@color-white-alpha-70",
        "black": "@color-black-alpha-95",
        "disabled": "@color-white-alpha-40",
        "success": {
          "normal": "@color-green-40",
          "hover": "@color-green-20"
        },
        "error": {
          "normal": "@color-red-40",
          "hover": "@color-red-20"
        },
        "warning": {
          "normal": "@color-yellow-40"
        },
        "secondary-light": "@color-white-alpha-70",
        "secondary-dark": "@color-black-alpha-60"
      },
      "overlay": {
        "primary": {
          "normal": "@color-black-alpha-90"
        },
        "secondary": {
          "normal": "@color-black-alpha-40"
        }
      },
      "button": {
        "primary": {
          "normal": "@color-white-alpha-95",
          "hover": "@color-white-alpha-80",
          "pressed": "@color-white-alpha-70",
          "disabled": "@color-white-alpha-20"
        },
        "secondary": {
          "normal": "@color-white-alpha-00",
          "hover": "@color-white-alpha-07",
          "disabled": "@color-white-alpha-00",
          "active": {
            "normal": "@color-white-alpha-20",
            "hover": "@color-white-alpha-30",
            "pressed": "@color-white-alpha-40"
          }
        },
        "secondary-dark": {
          "normal": "@color-black-alpha-00",
          "hover": "@color-black-alpha-07",
          "pressed": "@color-black-alpha-20",
          "disabled": "@color-black-alpha-00"
        },
        "join-animated-gradient": {
          "normal": ["@color-mint-40", "@color-blue-40"]
        }
      },
      "outline": {
        "button": {
          "normal": "@color-white-alpha-30"
        },
        "primary": {
          "normal": "@color-white-alpha-40"
        },
        "input": {
          "normal": "@color-white-alpha-50"
        },
        "disabled": {
          "normal": "@color-white-alpha-20"
        },
        "closed-caption": {
          "secondary": {
            "light": "@color-white-alpha-20",
            "dark": "@color-black-alpha-20"
          }
        }
      },
      "control": {
        "inactive": {
          "normal": "@color-black-alpha-20"
        },
        "shareContent": {
          "active": "@color-orange-60",
          "inactive": "@color-gray-60",
          "selected": "@color-green-40"
        }
      },
      "background": {
        "solid": {
          "secondary-dark": "@color-gray-95",
          "primary-light": "@color-white-alpha-100"
        },
        "alert-share": {
          "normal": "@color-orange-60"
        }
      },
      "touchbar": {
        "background": {
          "blue": {
            "normal": "@color-blue-60",
            "pressed": "@color-blue-80"
          },
          "pink": {
            "normal": "@color-pink-60",
            "pressed": "@color-pink-80"
          },
          "mint": {
            "normal": "@color-mint-60",
            "pressed": "@color-mint-80"
          },
          "purple": {
            "normal": "@color-purple-60",
            "pressed": "@color-purple-80"
          },
          "violet": {
            "normal": "@color-violet-60",
            "pressed": "@color-violet-80"
          },
          "orange": {
            "normal": "@color-orange-60",
            "pressed": "@color-orange-80"
          },
          "green": {
            "normal": "@color-green-60",
            "pressed": "@color-green-80"
          },
          "red": {
            "normal": "@color-red-60",
            "pressed": "@color-red-80"
          },
          "transparent": "@color-black-alpha-00"
        },
        "icon": {
          "stable": "@color-green-50",
          "attention": "@color-red-40",
          "secure": "@color-blue-40"
        }
      },
      "content": {
        "pdf": "@color-partner-pdf",
        "word": "@color-partner-word",
        "powerpoint": "@color-partner-powerpoint",
        "excel": "@color-partner-excel",
        "onenote": "@color-partner-onenote",
        "doc": "@color-partner-doc",
        "sheets": "@color-partner-sheets",
        "slides": "@color-partner-slides",
        "keynotes": "@color-partner-keynotes"
      }
    }
  }
}
